    "@types/cookie": "^0.4.0",
    "@types/jest": "^27.0.1",
    "autoprefixer": "^10.3.1",
    "cssnano": "^5.0.6",
    "esbuild": "^0.13.4",
    "eslint": "^7.22.0",
